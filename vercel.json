{
  "buildCommand": "npm run build",
  "outputDirectory": "dist/public",
  "env": {
    "VITE_SUPABASE_URL": "@supabase_url",
    "VITE_SUPABASE_ANON_KEY": "@supabase_key",
    "VITE_N8N_WEBHOOK_URL": "@n8n_webhook",
